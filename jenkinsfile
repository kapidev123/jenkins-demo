pipeline {
  agent any
  stages {
    stage('Checkout') {
      steps { git branch: 'main', url: 'https://github.com/<your_github_username>/jenkins-demo.git' }
    }
    stage('Build') {
      steps {
        sh 'echo "Building app..."'
        sh 'zip -r app.zip app.py'
      }
    }
    stage('Test') {
      steps { sh 'echo "Running tests..."' }
    }
    stage('Archive Artifact') {
      steps { archiveArtifacts artifacts: 'app.zip', fingerprint: true }
    }
  }
}
